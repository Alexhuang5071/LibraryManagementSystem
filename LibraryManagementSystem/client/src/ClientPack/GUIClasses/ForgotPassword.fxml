<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientPack.GUIClasses.ForgotPasswordController">
    <children>
        <Label layoutX="118.0" layoutY="119.0" text="Email:">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="Email_Field" layoutX="160.0" layoutY="119.0" prefWidth="180.0" />
        <Button fx:id="Submit_Button" layoutX="362.0" layoutY="117.0" mnemonicParsing="false" onAction="#Submit_Button_Click" text="Submit">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
      <Button layoutX="14.0" layoutY="461.0" mnemonicParsing="false" onAction="#Back_Button_Click" text="Back" />
      <Label layoutX="110.0" layoutY="43.0" text="Resetting Password">
         <font>
            <Font name="Bodoni MT Italic" size="36.0" />
         </font>
      </Label>
      <Label fx:id="EnterCodeLabel" layoutX="143.0" layoutY="173.0" text="Enter Verification Code Send to Email">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="Code_Field" layoutX="160.0" layoutY="193.0" prefWidth="180.0" />
      <Button fx:id="VerifyButton" layoutX="366.0" layoutY="191.0" mnemonicParsing="false" onAction="#Verify_Button_Click" text="Verify">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="ResendButton" layoutX="73.0" layoutY="191.0" mnemonicParsing="false" onAction="#Resend_Button_Click" text="Resend">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="CodeResent_Label" layoutX="223.0" layoutY="221.0" text="Code Resent!" visible="false" />
      <Label fx:id="CodeIncorrect_Label" layoutX="218.0" layoutY="221.0" text="Code Incorrect!" textFill="RED" visible="false" />
      <Label fx:id="CodeVerified_Label" layoutX="221.0" layoutY="221.0" text="Code Verified!" textFill="#08b119" visible="false" />
      <TextField fx:id="EnterPassField" layoutX="160.0" layoutY="270.0" prefWidth="180.0" />
      <Label fx:id="EnterPasswordLabel" layoutX="188.0" layoutY="250.0" text="Enter New Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="ResetButton" layoutX="223.0" layoutY="296.0" mnemonicParsing="false" onAction="#Reset_Button_Click" text="Reset">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="PasswordResetLabel" layoutX="200.0" layoutY="326.0" text="Password Reset!">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="LoginButton" layoutX="209.0" layoutY="384.0" mnemonicParsing="false" onAction="#Back_Button_Click" prefHeight="44.0" prefWidth="82.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="Required_Field" layoutX="80.0" layoutY="275.0" text="Required Field" textFill="#e70000" />
      <Label fx:id="EmailNotFound" layoutX="206.0" layoutY="147.0" text="Email Not Found" textAlignment="CENTER" textFill="#e70000" />
      <CheckBox fx:id="Show_Password" layoutX="354.0" layoutY="275.0" mnemonicParsing="false" onAction="#Show_Password_Check" text="Show Password" />
      <PasswordField fx:id="HiddenPasswordFIeld" layoutX="160.0" layoutY="271.0" prefHeight="25.0" prefWidth="180.0" />
    </children>
</AnchorPane>
